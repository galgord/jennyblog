{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/admin", "destination": "/index.html", "type": 200 },
    { "source": "/admin/*", "destination": "/index.html", "type": 200 }
  ],
  "rewrites": [
    { "source": "/**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/**",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
} 